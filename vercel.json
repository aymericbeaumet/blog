{
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Referrer-Policy",
					"value": "no-referrer"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				}
			]
		},
		{
			"source": "/:catchall+.md",
			"headers": [
				{
					"key": "Content-Type",
					"value": "text/plain; charset=utf-8"
				}
			]
		}
	],
	"github": {
		"silent": true
	}
}
