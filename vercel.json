{
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "https://aymericbeaumet.com"
				},
				{
					"key": "Content-Security-Policy",
					"value": "default-src 'self'; font-src 'fonts.googleapis.com' 'fonts.gstatic.com'; image-src '*.disquscdn.com'; style-src '*.disquscdn.com'; script-src: '*.disquscdn.com' 'disqus.com'"
				},
				{
					"key": "Referrer-Policy",
					"value": "no-referrer"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				}
			]
		},
		{
			"source": "/:catchall+.md",
			"headers": [
				{
					"key": "Content-Type",
					"value": "text/plain; charset=utf-8"
				}
			]
		}
	],
	"github": {
		"silent": true
	}
}
